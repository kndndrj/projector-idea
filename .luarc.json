{
    "runtime.version": "LuaJIT",
    "diagnostics": {
        "enable": true,
        "globals": [
            "vim"
        ],
        "disable": [
            "undefined-doc-class",
            "undefined-doc-name",
            "undefined-doc-param",
            "undefined-field"
        ]
    },
    "workspace": {
        "library": {
            "runtime/lua": true
        },
        "maxPreload": 2000,
        "preloadFileSize": 1000
    },
    "telemetry.enable": false
}
